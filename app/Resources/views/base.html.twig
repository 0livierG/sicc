<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">

    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">

    <link href='http://fonts.googleapis.com/css?family=Oswald:400,300' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>

    {% stylesheets filter='cssrewrite'  output='css/*.css'
    'bundles/siccfront/css/bootstrap.min.css'
    'bundles/siccfront/css/bootstrap-responsive.min.css'
    'bundles/siccfront/js/coin-slider/coin-slider-styles.css'
    'bundles/siccfront/css/ui-lightness/jquery-ui-1.10.3.custom.min.css'
    'bundles/siccfront/css/main.css'
    'bundles/sicccontact/css/style.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}"  type="text/css" media="all" />
    {% endstylesheets %}
</head>

<body>
<div class="hidden-phone hidden-md">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <div  id="logo-header">
                  {#<img src="{{ asset('bundles/siccfront/images/logo_sicc_simple.png') }}" title="SICC coordination SSI">#}
                    <div class='ligne1'><span>S.I.C.C</span> l'Incendie appliqué</div> <br>
                    <div class='ligne2'>Maîtrise d'oeuvre & Prévention incendie Audit/DIAG SSI Coordination SSI</div>
                </div>
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="nav-collapse collapse">
                    <ul class="nav">
                        <li><a href="{{ path('sicc_front_homepage') }}">Accueil</a></li>
                        <li><a href="{{ path('sicc_front_realisation') }}">Réalisations</a></li>
                        <li><a href="{{ path('sicc_front_article') }}">Articles</a></li>
                        <li><a href="{{ path('sicc_front_telechargement') }}">Téléchargement</a></li>
                        {% if is_granted('ROLE_USER') %}
                            <li><a href="{{ path('membre_edit', { 'id': app.user.id }) }}">Espace membres</a></li>
                        {% else %}
                            <li><a href="{{ path('fos_user_security_login') }}">Espace membres</a></li>
                        {%  endif %}
                        <li><a href="{{ path('sicc_contact') }}">Contact</a></li>

                    </ul>
                </div><!--/.nav-collapse -->
            </div>
        </div>
    </div>
</div>
<div id="mainWrapper">
    <div class="container">
        {% block body %}

        {% endblock %}
    </div>
</div>

{% block javascripts %}
    <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
    <script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script src="{{ asset('bundles/siccfront/js/jquery-ui-1.10.3.custom.min.js') }}"></script>
    <script src="{{ asset('bundles/siccfront/js/vendor/modernizr-2.6.1-respond-1.1.0.min.js') }}"></script>
    <script src="{{ asset('bundles/siccfront/js/coin-slider/coin-slider.js') }}"></script>
    <script src="{{ asset('bundles/siccfront/js/main.js') }}"></script>
{% endblock %}
<div id="container-footer-large">
    <div class="container">
        <footer>
            <div class="divider"></div>
            <div class="row">
                <div class="span2">
                    <h4>Naviguation</h4>
                    <ul>
                        <li>Accueil</li>
                        <li>Réalisation</li>
                        <li>Articles</li>
                    </ul>
                </div>
                <div class="span2">
                    <h4>Espace membre</h4>
                    <ul>
                        <li>Connexion</li>
                        <li>Enregistrement</li>
                        <li>Votre espace</li>
                    </ul>
                </div>
                <div class="span2">
                    <h4>Catégorie 3</h4>
                    <ul>
                        <li>Lorem Ipsum</li>
                        <li>Lorem Ipsum</li>
                        <li>Lorem Ipsum</li>
                    </ul>
                </div>
                <div class="span1"></div>
                <div id="copyright" class="span5">
                    <h5 class="">© Copyright 2013 S.I.C.C l'Incendie appliqué. All rights reserved.</h5>
                    <img class="icn-rs-footer" src="{{ asset('bundles/siccfront/img/icones/fb.png') }}">
                    <img class="icn-rs-footer" src="{{ asset('bundles/siccfront/img/icones/twitter.png') }}">
                    <img class="icn-rs-footer" src="{{ asset('bundles/siccfront/img/icones/google.png') }}">
                </div>
            </div>
        </footer>
    </div>
</div>
</body>
</html>
