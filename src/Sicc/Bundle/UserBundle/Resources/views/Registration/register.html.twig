{% extends "::admin_header.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}

{% block body %}


    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST"  role="form" style="width:400px;margin:auto;padding-top:20px;" class="fos_user_registration_register">
        {{ form_widget(form) }}
        <div>
            <input type="submit"  class="btn btn-lg btn-primary btn-block" value="{{ 'registration.submit'|trans }}" />
        </div>
    </form>

{% endblock %}
